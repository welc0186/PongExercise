; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="PongExercise"
run/main_scene="res://Scenes/title_screen.tscn"
config/features=PackedStringArray("4.1", "C#", "Mobile")
config/icon="res://icon.svg"

[autoload]

AudioManager="*res://Scenes/audio_manager.tscn"
GameConfig="*res://Scenes/game_config.tscn"

[display]

window/size/viewport_width=180
window/size/viewport_height=320
window/size/resizable=false
window/size/window_width_override=720
window/size/window_height_override=1280
window/stretch/scale=4.0

[dotnet]

project/assembly_name="PongExercise"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
